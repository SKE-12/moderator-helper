{
  "compilerOptions": {
    "target": "es6",                          
    "module": "commonjs",
    "jsx": "react",
    "sourceMap": true,
    "outDir": "build",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": true,
    "experimentalDecorators": true
  }
}
